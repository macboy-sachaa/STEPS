create or replace PROCEDURE USP_UP_MODEL_CONFIG (vMODEL_ID IN INTEGER, vSTEPCODE IN NVARCHAR2,vSTATUS IN NVARCHAR2) AS 
BEGIN
 
 UPDATE LOG_MODEL_CONFIG
 SET STATUS = vSTATUS
    ,IS_DONE = CASE WHEN UPPER(vSTATUS) IN ('COMPLETED','SUCCESS','DONE','SUCCESSFUL') THEN 1 ELSE 0 END
 WHERE MODEL_ID = vMODEL_ID
 AND STEP_CODE = vSTEPCODE;
 
 COMMIT;

END;