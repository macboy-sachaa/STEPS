create or replace PROCEDURE USP_UP_VERSION_STATUS(vC_VERSION NVARCHAR2,STEP_NAME NVARCHAR2,STATUSNAME NVARCHAR2) AS 
BEGIN
  
  
  UPDATE LOG_VERSION_STATUS
  SET EXECUTION_STATUS = STATUSNAME,
      LAST_UPDATED = SYSDATE
  WHERE TABLE_NAME = STEP_NAME
  AND C_VERSION = vC_VERSION
  AND EXECUTION_STATUS <> 'Done';
  
  COMMIT;
  
END USP_UP_VERSION_STATUS;